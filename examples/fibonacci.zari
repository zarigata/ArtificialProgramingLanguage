// Fibonacci sequence calculator
// Demonstrates recursion and basic arithmetic

fn fibonacci(n: u32) -> u32 {
    if n <= 1 {
        n
    } else {
        fibonacci(n - 1) + fibonacci(n - 2)
    }
}

fn main() {
    let n = 10;
    let result = fibonacci(n);
    println!("fibonacci({}) = {}", n, result);
}
